<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0"
           xmlns:mailappor16="http://schemas.microsoft.com/office/mailappversionoverrides/1.6"
           xsi:schemaLocation="http://schemas.microsoft.com/office/appforoffice/1.1 https://raw.githubusercontent.com/OfficeDev/office-js-docs-pr/main/docs/reference/manifest/schema/OfficeAppManifest_v1.1.xsd">
    <Id>9d65ef01-b3f7-4e7f-8c36-demo00000001</Id>
    <Version>1.0.0.0</Version>
    <ProviderName>CoffeeCup Tracker</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>
    <DisplayName DefaultValue="CoffeeCup Tracker"/>
    <Description DefaultValue="Track Outlook events in CoffeeCup"/>
    <IconUrl DefaultValue="https://cc-outlook-addin.vercel.app/icon.png"/>
    <SupportUrl DefaultValue="https://coffeecup.app"/>
    <AppDomains>
        <AppDomain>https://cc-outlook-addin.vercel.app</AppDomain>
    </AppDomains>
    <Hosts>
        <Host Name="Mailbox"/>
    </Hosts>
    <Requirements>
        <Sets DefaultMinVersion="1.3">
            <Set Name="Mailbox"/>
        </Sets>
    </Requirements>
    <DefaultSettings>
        <SourceLocation DefaultValue="https://cc-outlook-addin.vercel.app/taskpane.html"/>
    </DefaultSettings>
    <Permissions>ReadWriteItem</Permissions>

    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" Version="1.0">
        <Hosts>
            <Host xsi:type="MailHost">
                <DesktopFormFactor>
                    <FunctionFile resid="functionFile" />
                    <ExtensionPoint xsi:type="MessageReadCommandSurface">
                        <OfficeTab id="TabDefault">
                            <Group id="msgReadGroup">
                                <Label resid="groupLabel" />
                                <Control xsi:type="Button" id="msgReadOpenPaneButton">
                                    <Label resid="paneReadButtonLabel" />
                                    <Supertip>
                                        <Title resid="paneReadButtonTitle" />
                                        <Description resid="paneReadButtonDescription" />
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="icon16" />
                                        <bt:Image size="32" resid="icon32" />
                                        <bt:Image size="80" resid="icon80" />
                                    </Icon>
                                    <Action xsi:type="ShowTaskpane">
                                        <SourceLocation resid="taskpaneUrl" />
                                    </Action>
                                </Control>
                            </Group>
                        </OfficeTab>
                    </ExtensionPoint>
                </DesktopFormFactor>
            </Host>
        </Hosts>
        <Resources>
            <bt:Images>
                <bt:Image id="icon16" DefaultValue="https://cc-outlook-addin.vercel.app/icon16.png" />
                <bt:Image id="icon32" DefaultValue="https://cc-outlook-addin.vercel.app/icon32.png" />
                <bt:Image id="icon80" DefaultValue="https://cc-outlook-addin.vercel.app/icon80.png" />
            </bt:Images>
            <bt:Urls>
                <bt:Url id="functionFile" DefaultValue="https://cc-outlook-addin.vercel.app/functions.html" />
                <bt:Url id="taskpaneUrl" DefaultValue="https://cc-outlook-addin.vercel.app/taskpane.html" />
            </bt:Urls>
            <bt:ShortStrings>
                <bt:String id="groupLabel" DefaultValue="CoffeeCup Group" />
                <bt:String id="paneReadButtonLabel" DefaultValue="Open CoffeeCup Pane" />
                <bt:String id="paneReadButtonTitle" DefaultValue="Open the CoffeeCup tracking pane" />
            </bt:ShortStrings>
            <bt:LongStrings>
                <bt:String id="paneReadButtonDescription" DefaultValue="Track your emails with CoffeeCup" />
            </bt:LongStrings>
        </Resources>
    </VersionOverrides>
</OfficeApp>